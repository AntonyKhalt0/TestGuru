<%= form_with model:  [@test, @question] do |form| %>
  <%= render 'shared/errors', resource: @question %>
  <p>
      <%= form.label :body %><br>
      <%= form.text_field :body, id: :question_body %><br>  
  </p>

  <p>
    <%= form.hidden_field :test_id, value: params[:test_id]  %>
  </p>

  <p><%= form.submit %></p>
<% end %>
