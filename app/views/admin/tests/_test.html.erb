<tr>
  <td><%= test.title %></td>
  <td><%= test.level %></td>        
  <td><%= test.questions.count %></td>
  <td><%= button_to t('helpers.submit.test.show'), admin_test_path(test), method: :get %></td>
  <td><%= button_to t('helpers.submit.test.edit'), edit_admin_test_path(test), method: :get %></td>
  <td><%= button_to t('helpers.submit.test.delete'), admin_test_path(test), 
                                 method: :delete, 
                                 data: { confirm: "Are you sure?" } %></td>
</tr>
